{
    "name": "xboston/metahash",
    "description": "Pure PHP MetaHash library",
    "keywords": ["metahash", "blockchain"],
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/xboston/php-metahash",
    "require": {
        "php": ">=7.1",
        "ext-gmp": "*",
        "ext-json": "*",
        "mdanter/ecc": "^0.5.2",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Metahash\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "overtrue/phplint": "^1.1",
        "phpstan/phpstan": "^0.11",
        "squizlabs/php_codesniffer": "^3.4"
    }
}
