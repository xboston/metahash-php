{
    "name": "xboston/metahash",
    "description": "MetaHash Cryptocurrency PHP SDK",
    "keywords": [
        "metahash",
        "blockchain",
        "mhc",
        "cryptocurrency",
        "sdk"
    ],
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/xboston/php-metahash",
    "require": {
        "php": ">=8.0.2",
        "ext-gmp": "*",
        "ext-json": "*",
        "mdanter/ecc": "^1.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Metahash\\": "src/"
        }
    },
    "require-dev": {
        "overtrue/phplint": "^4.1",
        "phpstan/phpstan": "^1.4",
        "squizlabs/php_codesniffer": "^3.6",
        "ghostff/dump7": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    }
}
