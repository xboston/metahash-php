{
    "name": "xboston/metahash",
    "description": "MetaHash Cryptocurrency PHP SDK",
    "keywords": [
        "metahash",
        "blockchain",
        "mhc",
        "cryptocurrency",
        "sdk"
    ],
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/xboston/php-metahash",
    "require": {
        "php": ">=7.1",
        "ext-gmp": "*",
        "ext-json": "*",
        "mdanter/ecc": "^0.5.2",
        "guzzlehttp/guzzle": "^7"
    },
    "autoload": {
        "psr-4": {
            "Metahash\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2",
        "overtrue/phplint": "^2",
        "phpstan/phpstan": "^0.12",
        "squizlabs/php_codesniffer": "^3",
        "ghostff/dump7": "^1"
    }
}